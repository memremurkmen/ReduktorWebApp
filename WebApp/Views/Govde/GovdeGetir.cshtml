@model WebApp.Models.Entity.reduktorGovde

@{
    ViewBag.Title = "GovdeGetir";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Gövde Güncelle</h2>


@using (Html.BeginForm("GovdeGuncelle", "Govde", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        <label>ID</label>
        @Html.TextBoxFor(p => p.id, new { @class = "form-control", @readonly = "readonly" })
        <br />
        <label>GÖVDE ADI</label>
        @Html.TextBoxFor(p => p.govdeAdi, new { @class = "form-control" })
    </div>
    <br />
    <div>
        <button class="btn btn-warning">Güncelle</button>
    </div>
}