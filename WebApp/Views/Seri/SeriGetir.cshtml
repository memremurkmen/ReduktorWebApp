@model WebApp.Models.Entity.reduktorSeri

@{
    ViewBag.Title = "SeriGetir";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Seri Güncelleme</h2>


@using (Html.BeginForm("SeriGuncelle", "Seri", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        <label>ID</label>
        @Html.TextBoxFor(p => p.id, new { @class = "form-control", @readonly = "readonly" })
        <br />
        <label>SERİ ADI</label>
        @Html.TextBoxFor(p => p.seriAdi, new { @class = "form-control" })
    </div>
    <br />
    <div>
        <button class="btn btn-warning">Güncelle</button>
    </div>
}